.b90d713c.jpg\";","import { createContext, useReducer } from \"react\";\r\n\r\nexport const FilterContext = createContext();\r\n\r\nexport const FilterProvider = ({ children }) => {\r\n  const [state, dispatche] = useReducer(reducer, { sortBy: null,searchText:\"\",saveSearch:\"\",searched:false });\r\n  return (\r\n    <>\r\n      <FilterContext.Provider value={{ sortBy: state.sortBy, dispatche,searchText:state.searchText,saveSearch:state.saveSearch }}>\r\n        {children}\r\n      </FilterContext.Provider>\r\n    </>\r\n  );\r\n};\r\n\r\nfunction reducer(state, action) {\r\n  switch (action.type) {\r\n    case \"sort\":\r\n      return { ...state, sortBy: action.payload };\r\n   \r\n    case \"search\" :\r\n     \r\n      return {...state,searchText:action.payload};\r\n    case \"saveSearchText\":\r\n      return {...state,searchText:\"\",saveSearch:action.payload}\r\n    case \"searched\" :\r\n      return {...state,searched:action.payload}\r\n      default:\r\n        return state;\r\n      \r\n  }\r\n}\r\n","import {Link} from \"react-router-dom\"\r\nconst Navigation = ()=>\r\n{\r\n    return(\r\n        <>\r\n        <div  className=\"navigation sticky\">\r\n      \r\n            <ul className=\"navigation--list\">\r\n                <Link style={{textDecoration:\"none\",color:\"white\"}} to=\"sports\"><li>Sports Wear</li></Link>\r\n                <Link style={{textDecoration:\"none\",color:\"white\"}} to=\"gym\"><li>Gym Essentials</li></Link>\r\n                <Link style={{textDecoration:\"none\",color:\"white\"}} to=\"shoes\"><li>Shoes</li></Link>\r\n                <Link style={{textDecoration:\"none\",color:\"white\"}} to=\"supplements\"><li>Supplements</li></Link>\r\n                <Link style={{textDecoration:\"none\",color:\"white\"}} to=\"foodanddrink\"><li>Food And Drink</li></Link>\r\n            </ul>\r\n        </div>\r\n        </>\r\n\r\n    )\r\n}\r\nexport default Navigation","\r\nimport {Link} from \"react-router-dom\";\r\nimport {useCont